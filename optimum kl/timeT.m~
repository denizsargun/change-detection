classdef timeT
    % operation time test
    properties
        ex
        it
        method
        testName
        utility
    end
    
    methods
        function obj = timeT(method)
            obj.method = method;
            obj.ex = obj.method.ex;
            obj.it = obj.method.it(5);
            obj.testName = 'timeT';
            obj.utility = obj.method.utility;
        end
        
        function test(obj)
            if obj.it == 0
                return
            end
            
            totalTime = 0;
            for i = 1:obj.it
                dist = obj.utility.uniformly_random_dist();
                realDist = obj.utility.realize(dist);
                time = tic;
                numberOfDetections = numberOfDetections ...
                    +obj.method.is_change(dist);
            end
            
        end
        
    end
    
end